{
    "name": "spatie/laravel-settings",
    "description": "Store your application settings",
    "keywords": [
        "spatie",
        "laravel-settings"
    ],
    "homepage": "https://github.com/spatie/laravel-settings",
    "license": "MIT",
    "authors": [
        {
            "name": "Ruben Van Assche",
            "email": "ruben@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "doctrine/dbal": "^2.6|^3",
        "laravel/framework": "^6.0|^7.0|^8.24.0",
        "phpdocumentor/type-resolver": "^1.4",
        "spatie/data-transfer-object": "^1.0|^2.0",
        "spatie/temporary-directory": "^1.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.3|^1.4",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.5|^9.1",
        "predis/predis": "^1.1",
        "psalm/plugin-laravel": "^1.3|^1.4",
        "spatie/phpunit-snapshot-assertions": "^3.0|^4.2"
    },
    "suggest": {
        "predis/predis": "Flexible and feature-complete Redis client for PHP and HHVM",
        "ext-redis": "PHP extension for interfacing with redis"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\LaravelSettings\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\LaravelSettings\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm --no-diff --no-cache",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\LaravelSettings\\LaravelSettingsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
